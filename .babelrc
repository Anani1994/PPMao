{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    /*
     * 按需加载 Antd 模块
     *
     * yarn add babel-plugin-import --dev
     * less-loader:
     * {
     *   loader: "less-loader",
     *   options: {
     *     javascriptEnabled: true
     *   }
     * }
     * .babelrc or babel-loader option:
     * ["import", {
     *   "libraryName": "antd",
     *   "libraryDirectory": "es",
     *   "style": true
     * }]
     */
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "es",
      "style": true
    }]
  ]
}
